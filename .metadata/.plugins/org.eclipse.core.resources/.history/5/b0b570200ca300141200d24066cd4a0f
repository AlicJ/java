	@Test
	public void testEqualsObject() {
		assertTrue(!f12CAD.equals(null));
		assertEquals(f12CAD, new Money(12, "CAD"));
		assertEquals(f12CAD, f12CAD);
		assertTrue(!f12CAD.equals(f14CAD));
		assertTrue(!f12CAD.equals("f12CAD"));
	}

