package lab3;

public class Money {
	private int fAmount;
	private String fCurrency;
	
	public Money(int amount, String currency){
		this.fAmount = amount;
		this.fCurrency = currency;
	}
	
	public int amount() {
		return this.fAmount;
	}
	
	public String currency() {
		return fCurrency;
	}
	
	public Money add(Money m) {
		
	}
}
